{
  "name": "e-commerce",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,ts,tsx --fix",
    "preview": "vite preview",
    "format": "prettier --write src/**/*.{js,ts,tsx,scss,css,json}",
    "prepare": "cd .. && npx husky e-commerce/.husky",
    "lint:staged": "lint-staged",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "npx vitest --coverage"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,scss,css,json}": "prettier --write",
    "src/**/*.{js,ts,tsx}": "eslint --fix --max-warnings=0"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@reduxjs/toolkit": "^2.2.5",
    "axios": "^1.7.0",
    "npm": "^10.6.0",
    "pure-react-carousel": "^1.30.1",
    "react": "^18.2.0",
    "react-burger-menu": "^3.0.9",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.4",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.2.1",
    "react-modal": "^3.16.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.5",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-burger-menu": "^2.8.7",
    "@types/react-dom": "^18.2.22",
    "@types/react-modal": "^3.16.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.0",
    "c8": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "install": "^0.13.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "sass": "^1.76.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-sass-dts": "^1.3.18",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^1.5.3"
  }
}
